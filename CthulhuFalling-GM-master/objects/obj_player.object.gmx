        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Input
// Logic
move = obj_controlsManager.player_left + obj_controlsManager.player_right; //key_left + key_right;
hsp = move * movespeed;

// Facing
if (obj_controlsManager.player_right) {
   facing = 1;
} 

if (-obj_controlsManager.player_left) {
  facing = -1;
}

// Apply gravity
if (vsp &lt; 10) vsp += grav;

// Check sprint
if (obj_controlsManager.player_sprint) {
   sprinting = true;
}

if (sprinting and can_sprint) { 
   // Go faster
   movespeed = 4;
   
   // Set alarm to stop going faster
   if (set_sprint_alarm) {
      alarm[2] = sprint_steps;
      set_sprint_alarm = false;
   }
}

// Check jump
if (place_meeting(x, y + 1, obj_environmentItem)) {
    vsp = obj_controlsManager.player_jump * -jumpspeed;
    
    if (-obj_controlsManager.player_left) {
       if (obj_bag.bag[obj_bag.bag_slot] == noone)
          sprite_index = spr_newPlayerRunningLeft;
       else
           sprite_index = spr_newPlayerRunningLeftW;   
       
       if (not obj_controlsManager.player_jump) {
          walking = true;
       } else {
          walking = false;
       }
    } else if (obj_controlsManager.player_right) {
       if (obj_bag.bag[obj_bag.bag_slot] == noone)
          sprite_index = spr_newPlayerRunningRight;
       else
           sprite_index = spr_newPlayerRunningRightW;
       
       if (not obj_controlsManager.player_jump) {
          walking = true;
       } else {
          walking = false;
       }
    } else {
       if (obj_bag.bag[obj_bag.bag_slot] == noone)
          sprite_index = spr_newPlayerStanding;
       else
           sprite_index = spr_newPlayerStandingW;
       
       walking = false;
    }
} else {
  // Jump!
  if (obj_bag.bag[obj_bag.bag_slot] == noone)
     sprite_index = spr_newPlayerStanding;
  else
      sprite_index = spr_newPlayerStandingW;
}

if ((vsp &lt; 0) and (!obj_controlsManager.player_jump_held)) vsp = max(vsp, 0);

// Wall jump
if (obj_controlsManager.player_jump and walljump_enabled and (place_meeting(x + 7, y, obj_environmentItem) or place_meeting(x - 7, y, obj_environmentItem))) {
   vsp = -(jumpspeed * 1.2);
}

// Check climb items
if (place_meeting(x, y, obj_climbItem)) {
    vsp = obj_controlsManager.player_jump * -jumpspeed;

    if (obj_controlsManager.player_up) {
       vsp = -movespeed;
    }
   
    if (obj_controlsManager.player_down) {
       vsp = movespeed;
    }
}

// Inventory
/*if (mouse_wheel_up()) {
   // Increase current slot
   bag_slot = clamp(bag_slot++, 0, bag_size);
   
   // Set current item
   current_weapon = bag[bag_slot];
}

if (mouse_wheel_down()) {
   // Decrease current slot
   bag_slot = clamp(bag_slot--, 0, bag_size);
   
   // Set current item
   current_weapon = bag[bag_slot];
}*/

// Melee
if (obj_controlsManager.player_melee) {
   if (ENABLE_AUDIO)
      audio_play_sound(snd_woosh, 10, false);

   if (mouse_x &gt; obj_player.x) {
      sprite_index = spr_newPlayerPunchRight;
      with (instance_create(x + sprite_width, y + 10, obj_tentacle)) {
           image_xscale = 1;
      }
   }
   
   if (mouse_x &lt;= obj_player.x) {
      sprite_index = spr_newPlayerPunchLeft;
      with (instance_create(x, y + 10, obj_tentacle)) {
          image_xscale = -1; 
      }
   }
}

// Flashlight
if (obj_controlsManager.player_flashlight) {
   flashlight = true;
}

// Horizontal collision
if (place_meeting(x + hsp, y, obj_environmentItem)) {

    yplus = 0;
    while (place_meeting(x + hsp, y - yplus, obj_environmentItem) and yplus &lt;= abs(1 * hsp)) {
          yplus += 1;
    }

    if (place_meeting(x + hsp, y - yplus, obj_environmentItem)) {
        while (!place_meeting(x + sign(hsp), y, obj_environmentItem)) {
            x += sign(hsp);
        }
        hsp = 0;   
    } else {
        y -= yplus;
    }
}
x += hsp;

// Vertical collision
if (place_meeting(x, y + vsp, obj_environmentItem)) {
    while (!place_meeting(x, y + sign(vsp), obj_environmentItem)) {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;

// Play footsteps
/*
if (walking and place_meeting(x, y, obj_environmentItem) and not started_walking) {
   // Reset trigger
   started_walking = true;
   
   // Play sound
   audio_play_sound(snd_footstepsConcrete, 10, false);
   
   // Start alarm 1
   alarm[1] = 60;   
}
*/
</string>
